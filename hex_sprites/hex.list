005249r 1                  LDA SC_SELF_H,X               ;  2  59    recall downright high byte
005249r 1                  ADC SC_UPLEFT_H,X ;  4  63    add the uoleft high byte
005249r 1                  ROR ; 8x to 4x    ;  2  65
005249r 1                  ROR ZP_SELF_L     ;  5  70
005249r 1                  LSR ; 4x to 2x    ;  2  72
005249r 1                  ADC #0
